Config { font = "xft:Terminus:size=9:antialias=false"
       , bgColor = "black"
       , fgColor = "grey"
       , position = Top
       , lowerOnStart = True
       , commands = [ Run Network "wlan0" ["-L","0","-H","32","--normal","#4DA869","--high","#DD1144"] 10
                    , Run Cpu ["-L","3","-H","50","--normal","#4DA869","--high","#DD1144"] 10
                    , Run Memory ["-t","Mem: <usedratio>%"] 10
                    , Run Swap [] 10
                    , Run Com "uname" ["-s","-r"] "" 36000
                    , Run Date "%a %b %_d %H:%M" "date" 10
                    , Run BatteryP ["BAT0"] ["-l"
                                            , "#DD1144"
                                            , "-h"
                                            , "#4DA869"
                                            , "-t"
                                            , "<leftbar>"
                                            ] 20
                    , Run Wireless "wlan0" ["-t", "<essid>"] 600
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%cpu% | %memory% * %swap% | %wlan0% %wlan0wi%  }{ %uname% | %battery% | <fc=#9B859D>%date%</fc> "
       }
