urxvt*foreground: #ffffff
! urxvt*background: #000000

! Normal
! color0    black
! color1    red
! color2    green
! color3    yellow
! color4    blue
! color5    purple
! color6    cyan
! color7    white


urxvt*color8:    #4DA869
urxvt*color9:    #EF2929
urxvt*color10:    #BDA2BF
urxvt*color11:    #FFF391
urxvt*color12:    #7587A6
urxvt*color13:    #F0C47B
urxvt*color14:    #FF4040
urxvt*color15:    #EEEEEC
urxvt*color0:    #2E3436
urxvt*color1:    #DD1144
urxvt*color2:    #9B859D
urxvt*color3:    #F9EE98
urxvt*color4:    #424D5E
urxvt*color5:    #CDA869
urxvt*color6:    #E94444
